{
  "name": "polyfill-service",
  "description": "A polyfill combinator",
  "private": true,
  "contributors": [
    {
      "name": "Jake Champion",
      "email": "me@jakechampion.name",
      "url": "https://jakechampion.name"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JakeChampion/polyfill-service.git"
  },
  "bugs": {
    "url": "https://github.com/JakeChampion/polyfill-service/issues"
  },
  "scripts": {
    "fmt": "terraform fmt fastly/terraform/ && eslint . --fix",
    "lint": "eslint .",
    "clean": "rimraf ./dist",
    "build": "run-s clean eleventy build:*",
    "build:app": "js-compute-runtime app/index.js",
    "build:sass": "mkdir -p dist/css && sass src/assets/css/index.scss  --style=compressed --load-path=node_modules --load-path=node_modules/@financial-times | postcss --use autoprefixer --output dist/css/index.css --map",
    "build:js": "mkdir -p dist/js && esbuild src/assets/js/index.js --bundle --minify > dist/js/index.min.js",
    "dev": "npm run clean && nodemon --exec \"npm run build && npm run start\"",
    "eleventy": "eleventy --config .eleventy.js",
    "start": "viceroy bin/main.wasm -C fastly.toml",
    "test": "NODE_ENV='production' start-test start 'http://127.0.0.1:7878/v3/' test:integration",
    "test:integration": "mocha test/integration/*.test.js test/integration/**/*.test.js --recursive --check-leaks --timeout 60000 --exit --bail",
    "postinstall": "patch-package"
  },
  "license": "MIT",
  "dependencies": {
    "@fastly/js-compute": "^3.1.1",
    "@financial-times/o-message": "^5.4.2",
    "@financial-times/polyfill-useragent-normaliser": "^1.10.2",
    "@financial-times/useragent_parser": "^1.6.3",
    "@jakechampion/c-at-e-file-server": "^0.0.2-main",
    "hono": "^3.11.7",
    "mnemonist": "^0.39.5",
    "toposort": "^2.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@financial-times/math": "^1.1.0",
    "@financial-times/o-brand": ">=4.2.1 <5.0.0",
    "@financial-times/o-buttons": "^7.8.2",
    "@financial-times/o-colors": "^6.6.2",
    "@financial-times/o-editorial-typography": "^2.3.4",
    "@financial-times/o-fonts": "^5.3.4",
    "@financial-times/o-footer-services": "^4.2.6",
    "@financial-times/o-forms": "^9.11.2",
    "@financial-times/o-grid": "^6.1.7",
    "@financial-times/o-header-services": "^5.4.0",
    "@financial-times/o-icons": "^7.6.0",
    "@financial-times/o-layout": "^5.3.2",
    "@financial-times/o-loading": "^5.2.2",
    "@financial-times/o-normalise": "^3.3.1",
    "@financial-times/o-overlay": "^4.2.8",
    "@financial-times/o-quote": "^5.3.2",
    "@financial-times/o-spacing": "^3.2.3",
    "@financial-times/o-syntax-highlight": "^4.2.4",
    "@financial-times/o-table": "^9.3.1",
    "@financial-times/o-tabs": "^8.1.3",
    "@financial-times/o-tooltip": "^5.3.0",
    "@financial-times/o-typography": "^7.4.1",
    "@financial-times/o-utils": ">=2.2.0 <3.0.0",
    "@financial-times/o-viewport": "^5.1.1",
